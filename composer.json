{
    "name": "inweb/media",
    "description": "Media components for InWeb framework",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "media",
        "images"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.7",
        "spatie/once": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Media\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Media\\Tests\\": "tests/",
            "InWeb\\Media\\Database\\Seeds\\": "src/database/seeds/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Media\\MediaServiceProvider"
            ]
        }
    }
}
