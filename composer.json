{
    "name": "inweb/media",
    "description": "Media components for InWeb framework",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "media",
        "images"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "intervention/image": "^2.7",
        "laravel/legacy-factories": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Media\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Media\\Tests\\": "tests/",
            "InWeb\\Media\\Database\\Factories\\": "src/database/factories/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Media\\MediaServiceProvider"
            ]
        }
    }
}
