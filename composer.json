{
    "name": "inweb/media",
    "description": "Media components for InWeb framework",
    "keywords": [
        "framework",
        "laravel",
        "inweb",
        "media",
        "images"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "intervention/image": "^2.7",
        "laravel/legacy-factories": "^1.1",
        "php-ffmpeg/php-ffmpeg": "dev-master"
    },
    "require-dev": {
        "facade/ignition": "^2.17.6",
        "fakerphp/faker": "^v1.20.0",
        "laravel/sail": "^v1.15.1",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^v6.2.1",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Media\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Media\\Tests\\": "tests/",
            "InWeb\\Media\\Database\\Factories\\": "src/database/factories/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Media\\MediaServiceProvider"
            ]
        }
    }
}
